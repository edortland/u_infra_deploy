{
  "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
       "EnvironmentPrefix": {
      "type": "string",
       "allowedValues": [
        "usgt",
        "usgx",
        "usgp",
        "usgd"
      ],
      "metadata": {
        "description": "The prefix of the environment you are deploying"
      }
    },
    "SourceLocation": {
      "type": "string",
      "metadata": {
        "description": "The location of resources such as templates and DSC modules that the script is dependent"
      },
      "defaultValue": "https://raw.githubusercontent.com/edortland/u_infra_deploy/master/deploy_vnet"
    }
  },
  "variables": {
    "vnetTemplateUri" :  "[concat(parameters('SourceLocation'),'/templates/vnet.json')]"
  },
  "resources": [
    {
      "name": "VNet",
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[variables('vnetTemplateUri')]",
          "contentVersion": "1.0.0.0"
        },
        "parameters": {
          "location": {
            "value": "[resourceGroup().location]"
          },
          "EnvironmentPrefix": {
            "value": "[parameters('EnvironmentPrefix')]"
          }
        }
      }
    }
  ],
    "outputs": {
    }
}
